<project>
  <groupId>io.github.stephenc.dummy</groupId>
  <artifactId>testing-cd-with-maven</artifactId>
  <version>1.0-SNAPSHOT</version>
  <modelVersion>4.0.0</modelVersion>
  <packaging>pom</packaging>
  <scm>
    <developerConnection>scm:git:git@github.com:stephenc/testing-cd-with-maven.git</developerConnection>
  </scm>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-release-plugin</artifactId>
        <version>2.5.3</version>
        <configuration>
          <goals>verify</goals>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
